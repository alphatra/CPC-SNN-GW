{
  "model_name": "cpc_snn_gw",
  "batch_size": 2,
  "learning_rate": 0.001,
  "weight_decay": 0.0001,
  "num_epochs": 1,
  "num_classes": 2,
  "optimizer": "sgd",
  "scheduler": "cosine",
  "gradient_clipping": 1.0,
  "mixed_precision": true,
  "log_every": 10,
  "eval_every": 100,
  "save_every": 1000,
  "output_dir": "outputs",
  "use_wandb": true,
  "use_tensorboard": true,
  "project_name": "cpc-snn-gw",
  "max_memory_gb": 8.0,
  "enable_profiling": false,
  "early_stopping_patience": 10,
  "early_stopping_metric": "loss",
  "early_stopping_mode": "min",
  "surrogate_gradient_type": "SurrogateGradientType.ADAPTIVE_MULTI_SCALE",
  "curriculum_learning": true,
  "surrogate_adaptivity_factor": 2.0,
  "use_temporal_transformer": true,
  "transformer_num_heads": 2,
  "transformer_num_layers": 1,
  "multi_scale_kernels": [
    3,
    5
  ],
  "temporal_attention_dropout": 0.1,
  "use_learnable_thresholds": true,
  "num_threshold_scales": 2,
  "threshold_adaptation_rate": 0.01,
  "use_enhanced_lif": true,
  "use_refractory_period": true,
  "use_adaptation": true,
  "refractory_time_constant": 2.0,
  "adaptation_time_constant": 20.0,
  "use_momentum_negatives": true,
  "negative_momentum": 0.999,
  "hard_negative_ratio": 0.3,
  "curriculum_temperature": true,
  "cpc_latent_dim": 128,
  "snn_hidden_size": 64,
  "sequence_length": 256,
  "use_mixed_precision": true,
  "gradient_accumulation_steps": 1,
  "use_real_ligo_data": false,
  "num_samples": 20,
  "signal_noise_ratio": 0.4
}