{
  "model_name": "cpc_snn_gw",
  "batch_size": 16,
  "learning_rate": 0.001,
  "weight_decay": 0.0001,
  "num_epochs": 10,
  "num_classes": 2,
  "label_smoothing": 0.1,
  "use_class_weighting": true,
  "optimizer": "adamw",
  "scheduler": "cosine",
  "gradient_clipping": 1.0,
  "mixed_precision": true,
  "grad_accum_steps": 2,
  "log_every": 10,
  "eval_every": 100,
  "save_every": 1000,
  "output_dir": "outputs/h100_e60_b16_xai_pycbc/standard_training_16bs",
  "use_wandb": true,
  "use_tensorboard": false,
  "project_name": "gravitational-wave-detection",
  "max_memory_gb": 8.0,
  "enable_profiling": false,
  "early_stopping_patience": 10,
  "early_stopping_metric": "balanced_accuracy",
  "early_stopping_mode": "max",
  "checkpoint_every_epochs": 5,
  "use_focal_loss": true,
  "focal_gamma": 1.8,
  "class1_weight": 1.1,
  "use_ema": true,
  "ema_decay": 0.999,
  "spike_time_steps": 24,
  "spike_threshold": 0.1,
  "spike_learnable": true,
  "spike_threshold_levels": 4,
  "spike_surrogate_type": "adaptive_multi_scale",
  "spike_surrogate_beta": 4.0,
  "spike_pool_seq": false,
  "spike_target_rate_low": 0.1,
  "spike_target_rate_high": 0.2,
  "use_cpc_aux_loss": true,
  "cpc_aux_weight": 0.2,
  "ce_loss_weight": 1.0,
  "cpc_freeze_first_n_convs": 0,
  "cpc_prediction_steps": 12,
  "cpc_num_negatives": 128,
  "cpc_use_hard_negatives": true,
  "cpc_temperature": 0.07,
  "cpc_use_temporal_transformer": true,
  "cpc_attention_heads": 8,
  "cpc_transformer_layers": 4,
  "cpc_dropout_rate": 0.1,
  "cpc_use_grad_checkpointing": true,
  "cpc_use_mixed_precision": true,
  "snn_hidden_size": 32
}